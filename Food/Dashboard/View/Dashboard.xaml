<UserControl x:Class="Food.Dashboard.View.Dashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Food.Dashboard.View"
             xmlns:vm="clr-namespace:Food.Dashboard.ViewModel"
             mc:Ignorable="d">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="Привет, я доска!" FontWeight="Bold" Foreground="Wheat" FontSize="25"></TextBlock>
        <Button Content="Закрыть" Command="{Binding CloseCommand}"></Button>
    </StackPanel>
</UserControl>
